<div id="<%= dom_id probation_transfer_request %>">
  <p>
    <strong>Employee:</strong>
    <%= probation_transfer_request.employee_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= probation_transfer_request.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= probation_transfer_request.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= probation_transfer_request.phone %>
